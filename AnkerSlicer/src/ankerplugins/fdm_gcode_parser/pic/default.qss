

QPushButton{
    border: 0px;
    border-radius:4;
    background-color:#3A3B3F;
    color:#FFFFFF;
    font-size:16px
}
QPushButton:hover{
    color:#71D78A;
    border: 1px  solid #71D78A;
}

QPushButton:pressed {
    color:#71D78A;
    border: 1px  solid #71D78A;
}
QPushButton#checkButton{
    border: 0px;
    border-radius:4;
    background-color:#323437;
    color: #FFFFFF;
    font-size:12px
}

QPushButton#checkButton:hover{
    color:#71D78A;
    border: 1px  solid #71D78A;
}

QPushButton#checkButton:pressed {
    color:#71D78A;
    border: 1px  solid #71D78A;
}

QPushButton:disabled{
    background-color:#3A3B3F;
    color: #696969;
}
QPushButton#hightButton{
    background-color:#61D37D;
    color:#000000;
}
QPushButton#hightButton:hover{
    background-color:#71D78A;
}

QPushButton#hightButton:pressed{
    background-color:#61D37D;
}
QPushButton#hightButton:disabled{
    background-color:#3A3B3F;
    color: #696969;
}

QLabel {
    color:#FFFFFF
}

QListView {
    background-color:#323437 ;
    color:#FFFFFF;
}

QListView::item {
    border: 0px ;
    height:30px;
}

QListView#preferenceListView::item {
    border: 0px ;
    height: 50px ;
}

QListView::item:selected {
     background-color:#61D37D;
     color: #FFFFFF;
}

