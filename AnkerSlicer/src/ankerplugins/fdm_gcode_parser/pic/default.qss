QDialog#m_preferDialog {
    border: 0px;
    border-radius:30px;
    background-color:#292A2D;
}

QLabel#m_warning {
    color:#E32525
}

QCheckBox {
    color: #FFFFFF;
    background: #292A2D
}
/*选择框尺寸*/
QCheckBox::indicator{
    border: 1px solid #E7E7E9;
    width: 14px;
    height: 14px;
    color: #FFFFFF
}

QCheckBox::indicator::hover{
     border: 1px solid #61D37D;
}

QCheckBox::indicator::checked{
    image: url(:/images/icon/checkUpdate_icon_s.png)
}

QMenu {
    background-color: #3A3B3F;
    color:#FFFFFF;
    padding: 12px 0px 0px 12px;
    border-radius: 8px 8px 8px 8px;
}

QMenu::item{
    background-color: #3A3B3F;
    color:#FFFFFF
}

QMenu::item:selected{
    background-color: #61D37D;
}

